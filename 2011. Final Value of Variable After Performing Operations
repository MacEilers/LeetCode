class Solution {
    public int finalValueAfterOperations(String[] operations) {
        int ans = 0;
        for(int i =0; i<operations.length;i++){
            char helper= operations[i].charAt(1);
            if ((int )helper==43){
                ans++;
            }
            else {
                ans--;
            }
        }
        return ans ;
    }
}
